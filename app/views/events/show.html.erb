<div class="text-center">

<%= render @event %>

<%= link_to "Back to Calendar", web_events_calendar_path, class:"btn btn-primary mb-3" %>


<% if user_signed_in? %>
<div>
  <%= link_to "Edit this event", edit_event_path(@event), class:"btn btn-warning mb-3" %>

  <%= button_to "Destroy this event", @event, method: :delete, class:"btn btn-danger mb-3" %>
</div>
</div>
<% end %>

